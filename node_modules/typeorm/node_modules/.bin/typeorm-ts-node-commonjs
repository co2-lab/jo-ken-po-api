#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/caqsa/Desktop/jo-ken-po-api-main/node_modules/.pnpm/typeorm@0.3.20_mongodb@6.5.0_pg@8.11.5_ts-node@10.9.2/node_modules/typeorm/node_modules:/mnt/c/Users/caqsa/Desktop/jo-ken-po-api-main/node_modules/.pnpm/typeorm@0.3.20_mongodb@6.5.0_pg@8.11.5_ts-node@10.9.2/node_modules:/mnt/c/Users/caqsa/Desktop/jo-ken-po-api-main/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/caqsa/Desktop/jo-ken-po-api-main/node_modules/.pnpm/typeorm@0.3.20_mongodb@6.5.0_pg@8.11.5_ts-node@10.9.2/node_modules/typeorm/node_modules:/mnt/c/Users/caqsa/Desktop/jo-ken-po-api-main/node_modules/.pnpm/typeorm@0.3.20_mongodb@6.5.0_pg@8.11.5_ts-node@10.9.2/node_modules:/mnt/c/Users/caqsa/Desktop/jo-ken-po-api-main/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli-ts-node-commonjs.js" "$@"
else
  exec node  "$basedir/../../cli-ts-node-commonjs.js" "$@"
fi
